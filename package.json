{
    "name": "@frontify/frontify-authenticator",
    "version": "2.0.0-beta.0",
    "description": "Authenticate to your Frontify instance from within any secure web app.",
    "files": [
        "dist/**/*"
    ],
    "types": "dist/index.d.ts",
    "main": "dist/index.umd.js",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "module": "dist/index.es.js",
    "scripts": {
        "build": "npm run clean && cross-env NODE_ENV=production npm run build:ts",
        "typecheck": "tsc",
        "build:ts": "rollup -c",
        "clean": "rimraf dist",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Frontify/frontify-authenticator.git"
    },
    "keywords": [
        "frontify",
        "authenticator",
        "oauth",
        "brand"
    ],
    "author": "Frontify",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Frontify/frontify-authenticator/issues"
    },
    "homepage": "https://developer.frontify.com/",
    "devDependencies": {
        "@types/node": "^16.7.13",
        "cross-env": "^7.0.3",
        "esbuild": "^0.12.17",
        "esm": "^3.2.25",
        "node-fetch": "^3.0.0",
        "prettier": "^2.3.2",
        "requirejs": "^2.3.6",
        "rimraf": "^3.0.2",
        "rollup": "^2.55.1",
        "rollup-plugin-dts": "^3.0.2",
        "rollup-plugin-esbuild": "^4.5.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.3.5"
    }
}
